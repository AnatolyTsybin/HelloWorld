program HelloWorld;
begin
	writeln ('Hello, World!');
	writeln ('Here is the result of addition of 32 and 128: ', 32+128);
	writeln ('And the result of subtraction of the same numbers: ', 32-128);
	writeln ('Multiplication: ', 32*128);
	writeln ('Division: ', 32/128);
	writeln ('Integer division of 127 by 32: ', 127 div 32);
	writeln ('Remainder of integer division of 127 by 32: ', 127 mod 32)
end.
